{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/admin/login",
      "destination": "/index.html"
    },
    {
      "source": "/admin",
      "destination": "/index.html"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api/|_next/|_static/|_vercel/|favicon\\.ico|sitemap\\.xml|robots\\.txt).*)",
      "destination": "/index.html"
    }
  ]
}