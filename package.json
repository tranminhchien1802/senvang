{
  "name": "websenvang",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:fullstack": "concurrently \"npm run dev\" \"cd my-backend && npm run dev\"",
    "build": "vite build",
    "build:backend": "cd my-backend && npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "vite preview",
    "vercel-build": "vite build",
    "postinstall": "cd my-backend && npm install",
    "check-config": "node check-config.js",
    "check-deploy": "node check-deploy-config.js",
    "predeploy": "npm run check-config && npm run check-deploy",
    "deploy:vercel": "vercel --prod",
    "deploy:gh-pages": "npm run build && gh-pages -d dist",
    "docker:build-frontend": "docker build -f Dockerfile.frontend -t senvang-frontend .",
    "docker:build-backend": "docker build -f Dockerfile.backend -t senvang-backend .",
    "docker:run-frontend": "docker run -p 3000:80 senvang-frontend",
    "docker:run-backend": "docker run -p 5000:5000 senvang-backend"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@react-oauth/google": "^0.12.2",
    "axios": "^1.13.4",
    "dotenv": "^17.2.4",
    "i18next": "^25.7.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.67.0",
    "react-i18next": "^16.3.5",
    "react-router-dom": "^7.10.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "serve": "^14.2.5",
    "tailwindcss": "^4.1.17",
    "vite": "^7.2.4"
  }
}
